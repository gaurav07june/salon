<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval" >
            <solid android:color="@color/beautify_deep_light_black" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <corners android:radius="1dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval" >
            <padding
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp" />
            <solid android:color="@color/beautify_light_black" />
        </shape>
    </item>
</layer-list>