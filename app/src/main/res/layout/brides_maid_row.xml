<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

<LinearLayout

    android:layout_marginTop="@dimen/_5sdp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    >
    <com.beatutify.customviews.NMGTextView
        android:id="@+id/bridesmaid_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/txt_normal_size"
        android:text=""
        app:fontName="sf_ui_text_regular.otf"
        android:textColor="@color/beautify_deep_light_black"
        />
    <com.beatutify.customviews.NMGTextView
        android:id="@+id/bridesmaid_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/txt_normal_size"
        app:fontName="sf_ui_text_regular.otf"
        android:layout_marginLeft="@dimen/_15sdp"
        android:text=""
        android:textColor="@color/beautify_deep_light_black"
        />
    <com.beatutify.customviews.NMGTextView
        android:id="@+id/bridesmaid_service_type"
        android:layout_marginLeft="@dimen/_7sdp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/txt_normal_size"
        app:fontName="sf_ui_text_bold.otf"
        android:text=""
        android:textColor="@color/colorBlack"
        />



</LinearLayout>
</layout>