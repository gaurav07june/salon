<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>


    </data>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:background="@color/beautify_background_black"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rltlay_general_booking"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:paddingRight="@dimen/_10sdp"
                android:paddingLeft="@dimen/_10sdp"
                android:clickable="true"
                android:background="@drawable/layout_black_bg_mat">

                <ImageView
                    android:id="@+id/img_general_booking"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_height="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_calendar_white"/>
                <com.beatutify.customviews.NMGTextView
                    app:fontName="oswald_regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/general_booking"
                    android:textColor="@color/colorWhite"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:textSize="@dimen/label_text_size"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/img_general_booking"/>
                <ImageView
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_right_arrow"
                    android:id="@+id/img_general_booking_right_arrow"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:layout_height="@dimen/small_icon_size" />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/beautify_deep_light_black"
                />
            <RelativeLayout
                android:id="@+id/rltlay_marriage_booking"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:paddingRight="@dimen/_10sdp"
                android:paddingLeft="@dimen/_10sdp"
                android:clickable="true"
                android:background="@drawable/layout_black_bg_mat">

                <ImageView
                    android:id="@+id/img_marriage_booking"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_height="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_calendar_white"/>
                <com.beatutify.customviews.NMGTextView
                    app:fontName="oswald_regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/marriage_booking"
                    android:textColor="@color/colorWhite"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:textSize="@dimen/label_text_size"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/img_marriage_booking"/>
                <ImageView
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_right_arrow"
                    android:id="@+id/img_marriage_booking_right_arrow"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:layout_height="@dimen/small_icon_size" />

            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/beautify_deep_light_black"
                />
            <RelativeLayout
                android:id="@+id/rltlay_product_booking"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:paddingRight="@dimen/_10sdp"
                android:paddingLeft="@dimen/_10sdp"
                android:clickable="true"
                android:background="@drawable/layout_black_bg_mat">

                <ImageView
                    android:id="@+id/img_product_booking"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_height="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_calendar_white"/>
                <com.beatutify.customviews.NMGTextView
                    app:fontName="oswald_regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/product_booking"
                    android:textColor="@color/colorWhite"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:textSize="@dimen/label_text_size"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/img_product_booking"/>
                <ImageView
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_right_arrow"
                    android:id="@+id/img_product_booking_right_arrow"
                    android:layout_width="@dimen/small_icon_size"
                    android:layout_centerVertical="true"
                    android:layout_height="@dimen/small_icon_size" />

            </RelativeLayout>

        </LinearLayout>
    </RelativeLayout>
</layout>

